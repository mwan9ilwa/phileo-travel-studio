{
  "version": 2,
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "server/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/api/$1.ts"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    },
    {
        "src": "/[^.]+",
        "dest": "client/index.html",
        "status": 200
      }
  ]
}